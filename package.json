{
  "name": "kredos-automation",
  "version": "1.0.0",
  "description": "Kredos Application Test Automation",
  "main": "index.js",
  "scripts": {
    "test": "cucumber-js",
    "test:single": "cucumber-js --profile single",
    "test:set": "cucumber-js --profile set",
    "test:api": "playwright test tests/api/",
    "test:visual": "playwright test tests/visual/",
    "test:e2e": "cucumber-js --require-module tsconfig-paths/register --require-module ts-node/register --require tests/e2e/**/*.ts",
    "test:debug": "HEADLESS=false cucumber-js",
    "test:rcs-ui": "cucumber-js tests/e2e/features/rcs/rcs-tab.feature",
    "test:rcs-migration": "cucumber-js --profile rcs-migration",
    "test:regression": "cucumber-js --tags @regression",
    "test:regression:rcs": "cucumber-js --tags '@regression and @rcs'",
    "test:regression:critical": "cucumber-js --tags '@regression and @critical'",
    "test:regression:api": "cucumber-js --tags '@regression and @api'",
    "test:regression:ui": "cucumber-js --tags '@regression and @ui'",
    "test:active": "cucumber-js --tags @active",
    "test:dev-1059": "cucumber-js --profile dev-1059-all",
    "test:dev-1059:network-mock": "cucumber-js --profile dev-1059-network-mock",
    "test:dev-1059:real-environment": "cucumber-js --profile dev-1059-real-environment",
    "test:dev-1059:debug": "cucumber-js --profile dev-1059-debug",
    "test:rcs:ui-only": "HEADLESS=false cucumber-js --profile rcs-ui-only",
    "report:open": "npx playwright show-report reports/playwright-report",
    "xray:upload": "node tools/xray/integration/upload-feature-files.js",
    "xray:upload-by-feature": "node test_db_connection.js",
    "xray:sync": "ts-node tools/xray/sync/xray_sync_outreach_keys.ts",
    "xray:export": "node tools/xray/integration/export-results.js",
    "playwright:install": "npx playwright install",
    "playwright:codegen": "npx playwright codegen",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@types/chai": "^4.3.1",
    "@types/express": "^5.0.2",
    "@types/node": "^18.0.0",
    "@types/pg": "^8.0.0",
    "@types/ssh2": "^1.15.5",
    "@types/tunnel-ssh": "^5.0.4",
    "axios": "^0.27.2",
    "express": "^5.1.0",
    "form-data": "^4.0.2",
    "luxon": "^3.6.1",
    "pg": "^8.7.0",
    "ssh2": "^1.16.0",
    "ts-node": "^10.8.1",
    "tunnel-ssh": "^5.2.0",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^11.3.0",
    "@playwright/test": "^1.39.0",
    "@types/aws-sdk": "^0.0.42",
    "@types/cucumber": "^7.0.0",
    "@types/date-fns": "^2.5.3",
    "@types/jest": "^29.5.14",
    "allure-cucumberjs": "^3.2.2",
    "allure-js-commons": "^3.2.2",
    "aws-sdk": "^2.1692.0",
    "aws-sdk-client-mock": "^4.1.0",
    "chai": "^4.3.6",
    "chromedriver": "^136.0.2",
    "csv-writer": "^1.6.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "geckodriver": "^5.0.0",
    "husky": "^9.1.7",
    "mongodb": "^6.16.0",
    "mysql2": "^2.3.3",
    "selenium-webdriver": "^4.32.0",
    "ssh2-sftp-client": "^12.0.0",
    "tsconfig-paths": "^4.2.0"
  }
}

{
  "environments": {
    "staging": {
      "s3_bucket": "s3://kredos-uscellular-staging/kredos-uscellular-staging/ReturnSummary-File/",
      "sftp_path": "/staging/outreach-logs/"
    },
    "production": {
      "s3_bucket": "kredos-uscellular-production",
      "sftp_path": "/production/outreach-logs/"
    }
  },
  "file_generation": {
    "time": "17:52:00",
    "naming_pattern": "KAI_Kredos_outreach_log_YYYYMMDDHHMMSS.csv",
    "required_extensions": {
      "unencrypted": ".csv",
      "encrypted": ".csv.pgp"
    },
    "forbidden_extensions": [".crlf"]
  },
  "headers": [
    "ACCOUNTNUMBER",
    "FINANCIALACCOUNT",
    "TEMPLATENAME",
    "TEMPLATEMEMO",
    "TIMESTAMPCST-CDT",
    "CHANNEL",
    "EVENTNAME",
    "SMSCOPY"
  ],
  "validation": {
    "line_endings": {
      "required": "CRLF",
      "hex_value": "0D0A"
    },
    "file_structure": {
      "requires_headers": true,
      "requires_empty_end_row": true,
      "allow_empty_files": true
    }
  }
} 